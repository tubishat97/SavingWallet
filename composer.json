{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    
    "license": "MIT",
    "require": {
        "php": "^7.2.3|^8.0",
        "astrotomic/laravel-translatable": "^11.8",
        "awt/apilogger": "@dev",
        "bensampo/laravel-enum": "^2.0",
        "berkayk/onesignal-laravel": "^1.0",
        "brick/phonenumber": "^0.2.2",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.4",
        "firebase/php-jwt": "^5.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "intervention/image": "^2.5",
        "kawankoding/laravel-fcm": "^0.2.3",
        "kreait/firebase-php": "^5.14",
        "kreait/laravel-firebase": "^3.1",
        "laravel/framework": "7.*",
        "laravel/passport": "8.*",
        "laravel/tinker": "^2.5",
        "laravel/ui": "2.4",
        "laravelcollective/html": "^6.1",
        "lcobucci/jwt": "3.*",
        "macsidigital/laravel-zoom": "^4.1",
        "multicaret/laravel-unifonic": "^1.0",
        "pusher/pusher-php-server": "~4.0",
        "smartins/passport-multiauth": "^7.0",
        "spatie/laravel-permission": "^4.0",
        "tohidplus/laravel-vue-translation": "^2.1",
        "twilio/sdk": "^6.18",
        "xvladqt/faker-lorem-flickr": "dev-master"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "2.8",
        "facade/ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.3",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false
    },
    "extra": {
        "laravel": {
            "dont-discover": [
				"laravel/passport"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
